import React from 'react';
import Todo from './Todo'


const List = (props) => {
  

  return(
    <>
    <div className='list'>
    {props.todo.map((item) => (
              <Todo
              key={item.id}
              item={item}
              toggleList={props.toggleList}
              />
          ))}
                    
    </div>
    </>
  )
}

export default List